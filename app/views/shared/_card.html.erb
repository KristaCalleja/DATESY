<!-- This card receives a matchee (User) from MatchesController#new -->

<div class="container">
  <div class="card">
        <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/greece.jpg" class="card-img-top" alt="Matchee">
      <div class="card-body">
        <p class="card-name"><strong><%= matchee.name %></strong></p>
        <p class="card-text"> <%= matchee.description %></p>
        <p class="card-age"><%= matchee.age %> years old</p>
      </div>
  </div>
</div>
